{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "BasketItems Props",
  "description": "Props schema for the BasketItems component",
  "type": "object",
  "properties": {
    "items": {
      "type": "array",
      "description": "List of items to display in the basket",
      "items": {
        "type": "object",
        "properties": {
          "id": { "type": "string" },
          "type": { "type": "string", "enum": ["home", "location", "unknown"] },
          "description": { "type": "string" },
          "locked": { "type": "boolean" },
          "pinned": { "type": "boolean" }
        },
        "required": ["id", "type"]
      }
    },
    "onSearch": {
      "description": "Callback when search input changes",
      "instanceof": "Function"
    },
    "onFilter": {
      "description": "Callback when filter button is clicked",
      "instanceof": "Function"
    },
    "onClose": {
      "description": "Callback when close button is clicked",
      "instanceof": "Function"
    }
  },
  "required": []
}
